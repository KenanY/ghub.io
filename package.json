{
  "name": "ghub",
  "description": "npm-git.im/<package-name> takes you to the package's github page",
  "version": "0.2.3",
  "repository": {
    "type": "git",
    "url": "git://github.com/juliangruber/ghub.io.git"
  },
  "homepage": "http://ghub.io/",
  "main": "index.js",
  "scripts": {
    "test": "tap test/*.js",
    "start": "bin/ghub.io.js"
  },
  "bin": {
    "ghub.io": "./bin/ghub.io.js"
  },
  "dependencies": {
    "github-url": "~1.0.0",
    "npm-registry-client": "~0.2.18",
    "ecstatic": "~0.4.0",
    "ga": "0.0.2"
  },
  "devDependencies": {
    "webtap": "~0.1.0"
  },
  "keywords": [
    "npm",
    "git",
    "github",
    "website",
    "redirect"
  ],
  "author": {
    "name": "Julian Gruber",
    "email": "mail@juliangruber.com",
    "url": "http://juliangruber.com"
  },
  "license": "MIT",
  "engines": {
    "node": "0.8.x"
  },
  "subdomain": "ghub",
  "domains": [
    "ghub.io",
    "www.ghub.io"
  ],
  "bundledDependencies": [
    "github-url"
  ]
}
